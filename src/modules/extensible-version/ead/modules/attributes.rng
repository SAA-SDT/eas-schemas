            <data type="token"/>
         </attribute>
      </optional>
   </define>
   <define name="attribute-group.standard-date-attributes.optional"
           combine="interleave">
      <optional>
         <attribute name="notBefore">
            <data type="token"/>
         </attribute>
      </optional>
   </define>
   <define name="attribute-group.standard-date-attributes.optional"
           combine="interleave">
      <optional>
         <attribute name="standardDate">
            <data type="token"/>
         </attribute>
      </optional>
   </define>
   <define name="attribute.group.table-separators.optional">
      <optional>
         <attribute name="colsep">
            <data type="boolean"/>
         </attribute>
      </optional>
      <optional>
         <attribute name="rowsep">
            <data type="boolean"/>
         </attribute>
      </optional>
   </define>
</grammar>
