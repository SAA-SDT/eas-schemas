<xs:schema xmlns:ead="https://archivists.org/ns/ead/v4"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="https://archivists.org/ns/ead/v4">
   <xs:element name="ead">
      <xs:complexType>
         <xs:sequence>
            <xs:element type="ead:control" name="control"/>
            <xs:element type="ead:findAidDesc" name="findAidDesc" minOccurs="0"/>
            <xs:element type="ead:archDesc" name="archDesc"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="control">
      <xs:sequence>
         <xs:element type="ead:recordId" name="recordId"/>
         <xs:element type="ead:maintenanceAgency" name="maintenanceAgency"/>
         <xs:element type="ead:maintenanceHistory" name="maintenanceHistory"/>
         <xs:element type="ead:findAidDesc" name="findAidDesc" minOccurs="0"/>
         <xs:element type="ead:sources" name="sources" minOccurs="0"/>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element type="ead:conventionDeclaration" name="conventionDeclaration"/>
            <xs:element type="ead:languageDeclaration" name="languageDeclaration"/>
            <xs:element type="ead:localTypeDeclaration" name="localTypeDeclaration"/>
            <xs:element type="ead:otherRecordId" name="otherRecordId"/>
            <xs:element type="ead:rightsDeclaration" name="rightsDeclaration"/>
         </xs:choice>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="base" type="xs:anyURI"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="addressTypeLineEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherAddressTypeLineEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="agentTypeEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherAgentTypeEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="audienceEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherAudienceEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="contactTypeLineEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherContactTypeLineEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="countryEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso3166-1"/>
               <xs:enumeration value="otherCountryEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="coverageEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherCoverageEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="daoTypeEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherDaoTypeEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="detailLevelEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherDetailLevelEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="dscTypeEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherDscTypeEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="dateEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso8601"/>
               <xs:enumeration value="otherDateEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="languageEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso639-1"/>
               <xs:enumeration value="iso639-2"/>
               <xs:enumeration value="iso639-3"/>
               <xs:enumeration value="ietf-bcp-47"/>
               <xs:enumeration value="otherLanguageEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="levelEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherLevelEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="maintenanceEventTypeEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherMaintenanceEventTypeEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="maintenanceStatusEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherMaintenanceStatusEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="physDescStructuredTypeEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherPhysDescStructuredTypeEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="publicationStatusEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherPublicationStatusEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="repositoryEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso15511"/>
               <xs:enumeration value="otherRepositoryEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="scriptEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="iso15924"/>
               <xs:enumeration value="otherScriptEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="statusEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherStatusEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="targetTypeEncoding" use="required">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherTargetTypeEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="unitDateTypeEncoding">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="EASList"/>
               <xs:enumeration value="otherUnitDateTypeEncoding"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="detailLevel" type="xs:NMTOKEN"/>
      <xs:attribute name="maintenanceStatus" use="required" type="xs:token"/>
      <xs:attribute name="publicationStatus" type="xs:token"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="findAidDesc">
      <xs:choice maxOccurs="unbounded">
         <xs:element type="ead:agent" name="agent"/>
         <xs:element type="ead:citedRange" name="citedRange"/>
         <xs:element type="ead:date" name="date"/>
         <xs:element type="ead:div" name="div"/>
         <xs:element type="ead:place" name="place"/>
         <xs:element type="ead:title" name="title"/>
      </xs:choice>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="archDesc">
      <xs:sequence>
         <xs:element type="ead:did" name="did"/>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element type="ead:accessConditions" name="accessConditions"/>
            <xs:element type="ead:accruals" name="accruals"/>
            <xs:element type="ead:acqInfo" name="acqInfo"/>
            <xs:element type="ead:altFormAvail" name="altFormAvail"/>
            <xs:element type="ead:appraisal" name="appraisal"/>
            <xs:element type="ead:arrangement" name="arrangement"/>
            <xs:element type="ead:bibliography" name="bibliography"/>
            <xs:element type="ead:biogHist" name="biogHist"/>
            <xs:element type="ead:controlAccess" name="controlAccess"/>
            <xs:element type="ead:custodHist" name="custodHist"/>
            <xs:element type="ead:filePlan" name="filePlan"/>
            <xs:element type="ead:index" name="index"/>
            <xs:element type="ead:legalStatus" name="legalStatus"/>
            <xs:element type="ead:odd" name="odd"/>
            <xs:element type="ead:orginalsLoc" name="orginalsLoc"/>
            <xs:element type="ead:otherFindAid" name="otherFindAid"/>
            <xs:element type="ead:physTech" name="physTech"/>
            <xs:element type="ead:preferCite" name="preferCite"/>
            <xs:element type="ead:processInfo" name="processInfo"/>
            <xs:element type="ead:relatedMaterial" name="relatedMaterial"/>
            <xs:element type="ead:relations" name="relations"/>
            <xs:element type="ead:scopeContent" name="scopeContent"/>
            <xs:element type="ead:separatedMaterial" name="separatedMaterial"/>
            <xs:element type="ead:useConditions" name="useConditions"/>
         </xs:choice>
         <xs:element type="ead:dsc" name="dsc" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="base" type="xs:anyURI"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="recordId">
      <xs:simpleContent>
         <xs:restriction base="xs:anyType">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="(.|\n|\r)*\S(.|\n|\r)*"/>
               </xs:restriction>
            </xs:simpleType>
            <xs:attribute name="audience" type="xs:NMTOKEN"/>
            <xs:attribute name="id" type="xs:ID"/>
            <xs:attribute name="target" type="xs:IDREFS"/>
            <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
            <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
            <xs:anyAttribute namespace="##other" processContents="lax"/>
         </xs:restriction>
      </xs:simpleContent>
   </xs:complexType>
   <xs:complexType name="maintenanceAgency">
      <xs:sequence>
         <xs:choice>
            <xs:sequence>
               <xs:element type="ead:agencyCode" name="agencyCode"/>
               <xs:element type="ead:agencyName"
                           name="agencyName"
                           minOccurs="0"
                           maxOccurs="unbounded"/>
               <xs:element type="ead:otherAgencyCode"
                           name="otherAgencyCode"
                           minOccurs="0"
                           maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:sequence>
               <xs:element type="ead:agencyName" name="agencyName" maxOccurs="unbounded"/>
               <xs:element type="ead:otherAgencyCode"
                           name="otherAgencyCode"
                           minOccurs="0"
                           maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:choice>
         <xs:element type="ead:descriptiveNote" name="descriptiveNote" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="countryCode" type="xs:NMTOKEN"/>
      <xs:attribute name="vocabularySource" type="xs:token"/>
      <xs:attribute name="vocabularySourceURI" type="xs:anyURI"/>
      <xs:attribute name="valueURI" type="xs:anyURI"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="maintenanceHistory">
      <xs:sequence>
         <xs:element type="ead:maintenanceEvent"
                     name="maintenanceEvent"
                     maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="sources">
      <xs:sequence>
         <xs:element type="ead:source" name="source" maxOccurs="unbounded"/>
         <xs:element type="ead:descriptiveNote" name="descriptiveNote" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="base" type="xs:anyURI"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="conventionDeclaration">
      <xs:sequence>
         <xs:element type="ead:reference" name="reference"/>
         <xs:element type="ead:shortCode" name="shortCode" minOccurs="0"/>
         <xs:element type="ead:descriptiveNote" name="descriptiveNote" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="vocabularySource" type="xs:token"/>
      <xs:attribute name="vocabularySourceURI" type="xs:anyURI"/>
      <xs:attribute name="valueURI" type="xs:anyURI"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="languageDeclaration">
      <xs:sequence>
         <xs:element type="ead:descriptiveNote" name="descriptiveNote" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="languageCode" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptCode" type="xs:NMTOKEN"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="localTypeDeclaration">
      <xs:sequence>
         <xs:element type="ead:reference" name="reference"/>
         <xs:element type="ead:shortCode" name="shortCode" minOccurs="0"/>
         <xs:element type="ead:descriptiveNote" name="descriptiveNote" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="vocabularySource" type="xs:token"/>
      <xs:attribute name="vocabularySourceURI" type="xs:anyURI"/>
      <xs:attribute name="valueURI" type="xs:anyURI"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="otherRecordId" mixed="true">
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="localType" type="xs:token"/>
      <xs:attribute name="localTypeDeclarationReference" type="xs:IDREFS"/>
      <xs:attribute name="vocabularySource" type="xs:token"/>
      <xs:attribute name="vocabularySourceURI" type="xs:anyURI"/>
      <xs:attribute name="valueURI" type="xs:anyURI"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="rightsDeclaration">
      <xs:sequence>
         <xs:element type="ead:reference" name="reference"/>
         <xs:element type="ead:shortCode" name="shortCode" minOccurs="0"/>
         <xs:element type="ead:descriptiveNote" name="descriptiveNote" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="vocabularySource" type="xs:token"/>
      <xs:attribute name="vocabularySourceURI" type="xs:anyURI"/>
      <xs:attribute name="valueURI" type="xs:anyURI"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="agent" mixed="true">
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="vocabularySource" type="xs:token"/>
      <xs:attribute name="vocabularySourceURI" type="xs:anyURI"/>
      <xs:attribute name="valueURI" type="xs:anyURI"/>
      <xs:attribute name="agentType" use="required" type="xs:token"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="citedRange" mixed="true">
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="unit" type="xs:token"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="date" mixed="true">
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="localType" type="xs:token"/>
      <xs:attribute name="localTypeDeclarationReference" type="xs:IDREFS"/>
      <xs:attribute name="maintenanceEventReference" type="xs:IDREFS"/>
      <xs:attribute name="sourceReference" type="xs:IDREFS"/>
      <xs:attribute name="conventionDeclarationReference" type="xs:IDREFS"/>
      <xs:attribute name="calendar" type="xs:NMTOKEN"/>
      <xs:attribute name="certainty" type="xs:NMTOKEN"/>
      <xs:attribute name="era" type="xs:NMTOKEN"/>
      <xs:attribute name="notAfter" type="xs:token"/>
      <xs:attribute name="notBefore" type="xs:token"/>
      <xs:attribute name="standardDate" type="xs:token"/>
      <xs:attribute name="status">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="unknown"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="div">
      <xs:sequence>
         <xs:element type="ead:head" name="head" minOccurs="0"/>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element type="ead:list" name="list"/>
            <xs:element type="ead:p" name="p"/>
            <xs:element type="ead:chronList" name="chronList"/>
         </xs:choice>
         <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
               <xs:element type="ead:list" name="list"/>
               <xs:element type="ead:p" name="p"/>
               <xs:element type="ead:chronList" name="chronList"/>
            </xs:choice>
            <xs:element type="ead:div" name="div"/>
         </xs:sequence>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="localType" type="xs:token"/>
      <xs:attribute name="localTypeDeclarationReference" type="xs:IDREFS"/>
      <xs:attribute name="maintenanceEventReference" type="xs:IDREFS"/>
      <xs:attribute name="sourceReference" type="xs:IDREFS"/>
      <xs:attribute name="conventionDeclarationReference" type="xs:IDREFS"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="place">
      <xs:sequence>
         <xs:choice maxOccurs="unbounded">
            <xs:element type="ead:placeName" name="placeName"/>
            <xs:element type="ead:placeRole" name="placeRole"/>
            <xs:element type="ead:geographicCoordinates" name="geographicCoordinates"/>
            <xs:element type="ead:address" name="address"/>
            <xs:element type="ead:contact" name="contact"/>
         </xs:choice>
         <xs:choice minOccurs="0">
            <xs:element type="ead:date" name="date"/>
            <xs:element type="ead:dateRange" name="dateRange"/>
            <xs:element type="ead:dateSet" name="dateSet"/>
         </xs:choice>
         <xs:element type="ead:descriptiveNote" name="descriptiveNote" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="audience" type="xs:NMTOKEN"/>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="target" type="xs:IDREFS"/>
      <xs:attribute name="languageOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="scriptOfElement" type="xs:NMTOKEN"/>
      <xs:attribute name="localType" type="xs:token"/>
      <xs:attribute name="localTypeDeclarationReference" type="xs:IDREFS"/>
      <xs:attribute name="vocabularySource" type="xs:token"/>
      <xs:attribute name="vocabularySourceURI" type="xs:anyURI"/>
      <xs:attribute name="valueURI" type="xs:anyURI"/>
      <xs:attribute name="maintenanceEventReference" type="xs:IDREFS"/>
      <xs:attribute name="sourceReference" type="xs:IDREFS"/>
      <xs:attribute name="conventionDeclarationReference" type="xs:IDREFS"/>
      <xs:anyAttribute namespace="##other" processContents="lax"/>
   </xs:complexType>
   <xs:complexType name="title">
      <xs:sequence>
         <xs:element type="ead:part" name="part" maxOccurs="unbounded"/>
      </xs:sequence>
